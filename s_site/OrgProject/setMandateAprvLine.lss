Function setMandateAprvLine (doc As NotesDocument, readeritem As NotesItem) As Boolean
	
	On Error GoTo catch
	
	setMandateAppLine = False
	
	Dim orgdb As NotesDatabase
	Dim orgview As NotesView
	Dim orgdoccol As NotesDocumentCollection
	Dim orgdoc As NotesDocument
	
	Dim mdb As NotesDatabase
	Dim mview As NotesView
	Dim mdoc As NotesDocument
	
	Dim nm As NotesName
	Dim dummy As Variant
	Dim empno As String
	Dim sCode As String
	Dim sKey As String
	Dim tmptitle As String
	Dim nextappinfo As String
	Dim deptname As String
	Dim i As Integer
	
	Dim chgString As String
	Dim item As NotesItem
	Dim fd As Long
	Dim td As Long
	Dim cd As Long
	
	
	'사용자의 기본 정보와 겸직 정보를 모두 가져와서
	'현재의 deptname과 같은지를 체크 한 후에 deptcode
	'이렇게 하는게 그나마 정확해서.
	'deptname 은 lgcode 데이터베이스에서 같은 이름이 있음
	'그 상위의 부서명은 정보가 없음
	Set orgdb = New NotesDatabase("","")
	Call orgdb.Openwithfailover(doc.Parentdatabase.server, |syslib/lgorg.nsf|)
	If Not orgdb.isopen Then
		Exit Function
	End If
	Set orgview = orgdb.getView("DuoSelect")
	Set orgdoccol = orgview.getAllDocumentsByKey(doc.NextApprove(0))
	If orgdoccol.count = 0 Then
		Exit Function
	End If
	
	Set orgdoc = orgdoccol.Getfirstdocument()
	Do While Not orgdoc Is Nothing
		If orgdoc.DeptName(0) = doc.getItemValue("AppDept" & doc.NextAppChasu(0))(0) Then
			sCode=orgdoc.LGTDepCode(0)
			Exit Do
		End If
		Set orgdoc = orgdoccol.getNextDocument(orgdoc)
	Loop
	
	
	'대결정보에서 검색할 키 생성
	If sCode = "" Then
		Exit Function
	End If
	'dummy = Split(doc.NextApprove(0), " ")
	empno = orgdoc.LGTComnum(0)
	sKey = empno & |_| & sCode
	
	
	'대결정보 데이터베이스에서 문서 가져옴
	Set mdb = New NotesDatabase("","")
	Call mdb.Openwithfailover(doc.Parentdatabase.server, |syslib/mandate.nsf|)
	If Not mdb.isopen Then
		Exit Function
	End If
	Set mview = mdb.getView("vwGetMandateByKey")
	Set mdoc=mview.getDocumentByKey(sKey, True)
	
	
	cd=CLng(Format(Today, "yyyymmdd"))
	If Not mdoc Is Nothing Then
		fd=CLng(Replace(mdoc.fromdate(0), "-", ""))
		td=CLng(Replace(mdoc.todate(0), "-", ""))
		If (fd <= cd) And (td >= cd) Then
			Set nm=New NotesName(mdoc.MandateID(0))
			Set item = doc.getFirstItem("chgAprvLine")
			chgString = |Appr : | & doc.NextApprove(0) & | > | & nm.Common
			If item Is Nothing Then
				Set item = New NotesItem(doc, "chgAprvLine", "")
			End If
			Call item.Appendtotextlist(chgString)
			Call readerItem.Appendtotextlist(doc.NextApprove(0))
			
			doc.NextApprove=nm.Common
			doc.NextTitle=mdoc.mLGTGrade(0)
			
			setMandateAppLine = True
		End If
	End If
	
	Exit Function
catch:
	MsgBox "해외출장품의서 > 에이전트 > setMandateAppLine > " & Str(Erl) & " / " & Error
	Resume next_
next_: 
End Function


